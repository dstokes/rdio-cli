#! /usr/bin/env node

require('coffee-script');

var path   = require('path')
  , rdio   = require(path.join(__dirname, '..', 'lib/main.coffee'))
  , args   = process.argv.splice(2)
  , log    = console.log
  , action = (args.shift() || 'help');
  ;

if(rdio[action]) {
  rdio[action].apply(rdio, args);
}

